{
  "name": "closer",
  "description": "Closer.js - Clojure parser in JavaScript, compatible with the Mozilla Parser API (https://developer.mozilla.org/en-US/docs/SpiderMonkey/Parser_API)",
  "homepage": "http://github.com/vickychijwani/closer.js",
  "main": "closer.js",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.24"
  },
  "author": {
    "name": "Vicky Chijwani",
    "email": "vickychijwani@gmail.com",
    "url": "http://vickychijwani.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/vickychijwani/closer.js"
  },
  "scripts": {
    "pretest": "jison src/grammar.y src/lexer.l -o src/parser.js",
    "test": "jasmine-node spec --noStack --noColor --verbose",
    "watch": "npm-watch",
    "browserify": "jison src/grammar.y src/lexer.l -o src/parser.js; browserify -s closer src/closer.js > demo/js/closer.js; browserify -s closer_spec spec/closer_spec.js > demo/js/closer_spec.js",
    "publish-demo": "git subtree push --prefix demo origin gh-pages"
  },
  "watch": {
    "test": [
      "src/lexer.l",
      "src/grammar.y",
      "src/nodes.js",
      "src/closer.js",
      "spec/*.js"
    ],
    "browserify": [
        "src/lexer.l",
        "src/grammar.y",
        "src/nodes.js",
        "src/closer.js",
        "spec/*.js"
    ]
  },
  "keywords": [
    "parser",
    "parse",
    "clojure",
    "ast",
    "syntax",
    "spidermonkey",
    "acorn",
    "esprima"
  ],
  "devDependencies": {
    "jison": "^0.4.13",
    "browserify": "^3.38.0",
    "jasmine-node": "^1.14.2",
    "json-diff": "^0.3.1",
    "npm-watch": "0.0.0"
  }
}
